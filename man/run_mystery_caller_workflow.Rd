% Generated by hand based on R/run_mystery_caller_workflow.R
\name{run_mystery_caller_workflow}
\alias{run_mystery_caller_workflow}
\title{Run the end-to-end mystery caller workflow}
\usage{
run_mystery_caller_workflow(
  taxonomy_terms = NULL,
  name_data = NULL,
  phase1_data,
  lab_assistant_names,
  output_directory,
  phase2_data,
  quality_check_path,
  phase1_output_directory = output_directory,
  split_insurance_order = c("Medicaid", "Blue Cross/Blue Shield"),
  phase2_required_strings = c(
    "physician_information", "able_to_contact_office", "are_we_including",
    "reason_for_exclusions", "appointment_date", "number_of_transfers",
    "call_time", "hold_time", "notes", "person_completing",
    "state", "npi", "name"
  ),
  phase2_standard_names = c(
    "physician_info", "contact_office", "included_in_study",
    "exclusion_reasons", "appt_date", "transfer_count",
    "call_duration", "hold_duration", "notes", "completed_by",
    "state", "npi", "name"
  ),
  npi_search_args = list(),
  all_states = NULL,
  verbose = interactive(),
  npi_progress_observer = NULL
)
}
\arguments{
\item{taxonomy_terms}{Character vector of taxonomy descriptions to pass to \code{\link[=search_by_taxonomy]{search_by_taxonomy}}(). Set to \code{NULL} to skip taxonomy-based searches.}

\item{name_data}{Optional data frame containing \code{first} and \code{last} columns to use with \code{\link[=search_and_process_npi]{search_and_process_npi}}(). Provide \code{NULL} to skip name-based searches.}

\item{phase1_data}{Data frame holding Phase 1 calling roster information to pass to \code{\link[=clean_phase_1_results]{clean_phase_1_results}}().}

\item{lab_assistant_names}{Character vector of caller names used when splitting the cleaned roster via \code{split_and_save()}. Must contain at least two entries.}

\item{output_directory}{Directory where \code{split_and_save()} should write the complete and per-caller workbooks.}

\item{phase2_data}{Data frame or file path consumed by \code{clean_phase_2_data()}.}

\item{quality_check_path}{File path where \code{save_quality_check_table()} should write the quality check CSV.}

\item{phase1_output_directory}{Directory where \code{clean_phase_1_results()} should write the cleaned Phase 1 CSV. Defaults to \code{output_directory}.}

\item{split_insurance_order}{Ordering passed to \code{split_and_save()}'s \code{insurance_order} argument. Defaults to \code{c("Medicaid", "Blue Cross/Blue Shield")}.}

\item{phase2_required_strings}{Character vector of substrings used when standardising Phase 2 column names via \code{clean_phase_2_data()}.}

\item{phase2_standard_names}{Replacement names corresponding to \code{phase2_required_strings}.}

\item{npi_search_args}{Named list of additional arguments forwarded to \code{search_and_process_npi()}.}

\item{all_states}{Optional character vector of all states to supply to \code{states_where_physicians_were_NOT_contacted()}.}

\item{verbose}{Logical. When \code{TRUE}, print stage updates to the console while running the workflow. Defaults to \code{interactive()}.}

\item{npi_progress_observer}{Optional callback that receives progress updates from \code{search_and_process_npi()}. It is invoked with the same payload as that function's \code{progress_callback} argument.}
}
\value{
A list containing intermediate artefacts from each workflow stage: \code{roster}, \code{validated_roster}, \code{cleaned_phase1}, \code{cleaned_phase2}, \code{coverage_summary}, and \code{quality_check_table}.
}
\description{
This helper orchestrates the core steps required to prepare and execute a mystery caller campaign. It stitches together roster creation, NPI validation, call sheet preparation, workload splitting, and Phase 2 hygiene checks so teams can focus on execution instead of plumbing.
}
